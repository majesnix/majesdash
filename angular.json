{
  "version": 1,
  "projects": {
    "api": {
      "$schema": "../../node_modules/nx/schemas/project-schema.json",
      "root": "apps/api",
      "sourceRoot": "apps/api/src",
      "projectType": "application",
      "architect": {
        "build": {
          "builder": "@nrwl/node:webpack",
          "outputs": [
            "{options.outputPath}"
          ],
          "options": {
            "outputPath": "dist/apps/api",
            "main": "apps/api/src/main.ts",
            "tsConfig": "apps/api/tsconfig.app.json",
            "assets": [
              "apps/api/src/assets"
            ]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/api/src/environments/environment.ts",
                  "with": "apps/api/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@nrwl/node:node",
          "options": {
            "buildTarget": "api:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "outputs": [
            "{options.outputFile}"
          ],
          "options": {
            "lintFilePatterns": [
              "apps/api/**/*.ts"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": [
            "coverage/apps/api"
          ],
          "options": {
            "jestConfig": "apps/api/jest.config.ts",
            "passWithNoTests": true
          }
        }
      },
      "tags": []
    },
    "backend": {
      "$schema": "../../node_modules/nx/schemas/project-schema.json",
      "root": "libs/backend",
      "sourceRoot": "libs/backend/src",
      "projectType": "library",
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "outputs": [
            "{options.outputFile}"
          ],
          "options": {
            "lintFilePatterns": [
              "libs/backend/**/*.ts"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": [
            "coverage/libs/backend"
          ],
          "options": {
            "jestConfig": "libs/backend/jest.config.ts",
            "passWithNoTests": true
          }
        }
      },
      "tags": []
    },
    "data": {
      "$schema": "../../node_modules/nx/schemas/project-schema.json",
      "root": "libs/data",
      "sourceRoot": "libs/data/src",
      "projectType": "library",
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "outputs": [
            "{options.outputFile}"
          ],
          "options": {
            "lintFilePatterns": [
              "libs/data/**/*.ts"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": [
            "coverage/libs/data"
          ],
          "options": {
            "jestConfig": "libs/data/jest.config.ts",
            "passWithNoTests": true
          }
        }
      },
      "tags": []
    },
    "majesdash": {
      "$schema": "../../node_modules/nx/schemas/project-schema.json",
      "projectType": "application",
      "root": "apps/majesdash",
      "sourceRoot": "apps/majesdash/src",
      "prefix": "majesdash",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "outputs": [
            "{options.outputPath}"
          ],
          "options": {
            "outputPath": "dist/apps/majesdash",
            "index": "apps/majesdash/src/index.html",
            "main": "apps/majesdash/src/main.ts",
            "polyfills": "apps/majesdash/src/polyfills.ts",
            "tsConfig": "apps/majesdash/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "apps/majesdash/src/favicon.png",
              "apps/majesdash/src/assets"
            ],
            "styles": [
              "apps/majesdash/src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "30kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "apps/majesdash/src/environments/environment.ts",
                  "with": "apps/majesdash/src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "majesdash:build",
            "proxyConfig": "apps/majesdash/proxy.conf.json"
          },
          "configurations": {
            "production": {
              "browserTarget": "majesdash:build:production"
            },
            "development": {
              "browserTarget": "majesdash:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "majesdash:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/majesdash/src/**/*.ts",
              "apps/majesdash/src/**/*.html"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": [
            "coverage/apps/majesdash"
          ],
          "options": {
            "jestConfig": "apps/majesdash/jest.config.ts",
            "passWithNoTests": true
          }
        }
      },
      "tags": []
    },
    "majesdash-e2e": {
      "$schema": "../../node_modules/nx/schemas/project-schema.json",
      "root": "apps/majesdash-e2e",
      "sourceRoot": "apps/majesdash-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/majesdash-e2e/cypress.config.ts",
            "browser": "chrome"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "outputs": [
            "{options.outputFile}"
          ],
          "options": {
            "lintFilePatterns": [
              "apps/majesdash-e2e/**/*.{js,ts}"
            ]
          }
        }
      },
      "tags": [],
      "implicitDependencies": [
        "majesdash"
      ]
    },
    "majesdash-e2e-chrome": {
      "$schema": "../../node_modules/nx/schemas/project-schema.json",
      "root": "apps/majesdash-e2e",
      "sourceRoot": "apps/majesdash-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/majesdash-e2e/cypress.config.ts",
            "browser": "chrome"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "outputs": [
            "{options.outputFile}"
          ],
          "options": {
            "lintFilePatterns": [
              "apps/majesdash-e2e/**/*.{js,ts}"
            ]
          }
        }
      },
      "tags": [],
      "implicitDependencies": [
        "majesdash"
      ]
    },
    "majesdash-e2e-firefox": {
      "$schema": "../../node_modules/nx/schemas/project-schema.json",
      "root": "apps/majesdash-e2e",
      "sourceRoot": "apps/majesdash-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/majesdash-e2e/cypress.config.ts",
            "browser": "firefox"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "outputs": [
            "{options.outputFile}"
          ],
          "options": {
            "lintFilePatterns": [
              "apps/majesdash-e2e/**/*.{js,ts}"
            ]
          }
        }
      },
      "tags": [],
      "implicitDependencies": [
        "majesdash"
      ]
    },
    "web": {
      "$schema": "../../node_modules/nx/schemas/project-schema.json",
      "projectType": "library",
      "root": "libs/web",
      "sourceRoot": "libs/web/src",
      "prefix": "majesdash",
      "architect": {
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": [
            "coverage/libs/web"
          ],
          "options": {
            "jestConfig": "libs/web/jest.config.ts",
            "passWithNoTests": true
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/web/src/**/*.ts",
              "libs/web/src/**/*.html"
            ]
          }
        }
      },
      "tags": []
    }
  }
}
