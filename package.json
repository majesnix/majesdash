    "@majesnix/color-picker": "14.0.1",
    "@majesnix/file-input": "14.0.1",
    "@nestjs/common": "9.0.11",
    "@nestjs/config": "^1.1.5",
    "@nestjs/core": "9.0.11",
    "@nestjs/platform-express": "9.0.11",
